{
  "manifest_version": 2,

  "name": "Women in Tech",
  "description": "A project for the Women in Tech Demo Days Hackathon",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "js": ["clarifai.js", "d3.js"],
      "matches": ["https://www.linkedin.com/mynetwork/*", "http://*.d3.js/*", "https://*.d3.js/*"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
